pipeline {
agent {
  label 'slavenode1-maven'
}

stages {
  stage('GIT-CLONE') {
    steps {
      git branch: 'main', credentialsId: 'git', url: 'https://github.com/Vimala-Daniel/devops.git'
    }
   }	
  stage('MAVEN-BUILD') {
    steps {
    sh '''mvn clean install
    '''
	     }
    }
  } 
}  
